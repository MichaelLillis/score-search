{
  "name": "sportsbot",
  "version": "5.7.1",
  "main": "index.js",
  "author": "TylerJenningsW <tylerjennings2@outlook.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "dev:main": "nodemon src/main.ts",
    "register-guild-commands": "ts-node src/register-guild-commands.ts",
    "dev": "run-s register-guild-commands dev:main",
    "start": "node build/main.js",
    "test": "jest"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "nodemon": "^1.3.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@discordjs/builders": "0.15.0",
    "@discordjs/rest": "0.5.0",
    "@types/puppeteer": "^5.4.6",
    "discord-api-types": "0.35.0",
    "discord.js": "^13.8.1",
    "dotenv": "^16.0.1",
    "puppeteer": "^15.1.1"
  }
}
